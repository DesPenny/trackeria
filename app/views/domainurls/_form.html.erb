<%= simple_form_for(@domainurl) do |f| %>
  <%= f.error_notification %>
    <div class="form-inputs">
    <%= f.input :domainurl, :label=>"URL" %>
    <%= f.input :keyword %>
    <%= f.input :category, :required=>:false %>
    </div>
  
    <div class="form-actions">
    <%= f.button :submit, "Update" %>
    </div>
<% end %>





